= content header
  {{ partial "subnav" . }}
= content main
  .container.m-auto.mt-8 class="md:flex-row max-w-xl"
    h1 {{ .Title }}
    .mt-8
      h2.text-2xl Library Comparisons
      p Articles and Resources about different libraries.  
      .mt-6
        {{ range where .Site.Taxonomies.categories.resource "Params.section" "comparison" }}
          {{ partial "resource-item" . }}
        {{ end }}
    {{.Content}}
= content modal
  .modal
    {{ if .Page.Params.modal }}
      {{ partial (.Page.Param "modal.template") . }}
    {{ end }}
